module win(

  input logic [15:0][3:0] cell_matrix_in,
  output logic win, a, b, c, d
  
  );

  // Definir un array temporal para almacenar los valores de la matriz
  logic [15:0] temp[0:15];
  integer i;
  logic temp_win = {a,b,c,d};

  always_comb begin
  
	 temp_win = 0;
  
    for (int i = 0; i < 16; i++) begin
      temp[i] = cell_matrix_in[i];
    end
	 
	 for (int i = 0; i < 16; i++) begin
      if(cell_matrix_in[i] == temp_win) begin
			temp_win = 1;
		end
    end
   
	 win = temp_win;
	 
  end

endmodule